brew update
brew install mysql
brew services start mysql

mysqladmin -u root -p version

mysql -u root -p


show databases;

create database name;

use name;

show tables;

create table name(
id int,
name varchar(255)
);


describe name;
insert into name values (1, "name");
select * from name;

select id from tablename where name = "atharv";

select id from tablename where name = "atharv" or name = "atharvm";

select name from tablename where age < 22;

select id from tablename where not name = "atharv";


delete from tablename where name = "atharv";

update tablename set email = NULL where name = "atharv";


select * from tablename order by age asc;

select * from tablename order by age desc;


alter table tablename add phone_number int;

update tablename set phone_number = 1234567890 where name = "atharv";
