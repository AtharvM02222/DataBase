use brycxizr5tfdlzufdy1x

show databases;

create database name;

use name;

show tables;

create table name(
id int,
name varchar(255)
);


describe name;
insert into name values (1, "name");
select * from name;

select id from RX where name = "Atharv Mandlavdiya";

select id from RX where name = "Atharv Mandlavdiya" or name = "atharv";

select name from RX where age < 22;

select id from RX where not name = "Atharv Mandlavdiya";


delete from RX where name = "Atharv Mandlavdiya";

update RX set email = NULL where name = "Atharv Mandlavdiya";


select * from RX order by age asc;

select * from RX order by age desc;


alter table RX add phone_number varchar(15);

update RX set phone_number = "99931-00837" where name = "Atharv Mandlavdiya";
